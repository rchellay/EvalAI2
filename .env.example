# Backend environment example
SECRET_KEY=change_me_in_production
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Database (choose either direct DATABASE_URL or individual POSTGRES_* to compose it)
DATABASE_URL=sqlite:///./eduapp.db
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=eduapp
POSTGRES_USER=eduuser
POSTGRES_PASSWORD=edupassword

# CORS (comma separated origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Auto create tables (only for sqlite dev). Set to 0 in Postgres environments.
RUN_SYNC_DB=1

# Alembic
ALEMBIC_SCHEMA=public
